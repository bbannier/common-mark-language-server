au User lsp_setup call lsp#register_server({
            \ 'name': 'common-mark',
            \ 'cmd': {server_info->['cargo', 'run']},
            \ 'whitelist': ['markdown'],
            \ })

autocmd FileType markdown setlocal omnifunc=lsp#complete
set foldmethod=expr
            \ foldexpr=lsp#ui#vim#folding#foldexpr()
            \ foldtext=lsp#ui#vim#folding#foldtext()
